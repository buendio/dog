    {%- endfor -%}

    {%- assign review_data_array = array_builder | split:'_' -%}
    {%- assign g_review_count = review_data_array[0] -%}
    {%- assign g_rating_value = review_data_array[1] -%}
{%- endif -%}
{%- comment -%}
  For Judge Reviews
{%- endcomment -%}
{%- if schemaProduct.metafields.judgeme.badge -%}
